{
  "extends": "markdownlint/style/prettier",
  "default": true,
  "MD013": false,
  "MD033": false,
  "MD041": false,
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD005": true,
  "MD007": {
    "indent": 4,
    "start_indented": false
  },
  "MD010": true,
  "MD011": true,
  "MD012": {
    "maximum": 1
  },
  "MD014": true,
  "MD018": true,
  "MD019": true,
  "MD020": true,
  "MD021": true,
  "MD022": {
    "lines_above": 1,
    "lines_below": 1
  },
  "MD023": true,
  "MD024": {
    "siblings_only": false
  },
  "MD025": {
    "level": 1,
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "MD026": {
    "punctuation": ".,;:!。，；：！"
  },
  "MD027": {
    "list_items": true
  },
  "MD028": true,
  "MD029": {
    "style": "one_or_ordered"
  },
  "MD030": {
    "ul_single": 3,
    "ul_multi": 3,
    "ol_single": 1,
    "ol_multi": 1
  },
  "MD031": {
    "list_items": true
  },
  "MD032": true,
  "list-marker-space": {
    "ul_multi": 3,
    "ul_single": 3
  },
  "ul-indent": {
    "indent": 4
  }
}
